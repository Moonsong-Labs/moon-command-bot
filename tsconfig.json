{
  "compilerOptions": {
    "target": "esnext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "build",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "composite": true,
  },
  "include": ["src/**/*.ts"],
  "exclude": ["src/**/*.spec.ts", "node_modules"]
}
